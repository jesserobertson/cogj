<?xml version="1.0" encoding="UTF-8"?>
<WFS_Capabilities version="1.0.0" xmlns="http://www.opengis.net/wfs" xmlns:cite="http://www.opengeospatial.net/cite" xmlns:ogc="http://www.opengis.net/ogc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/wfs">
	<Service>
		<Name>
			{SERVICE_TITLE}
		</Name>
		<Title>
			{SERVICE_TITLE}
		</Title>
		<Abstract>
			Geo, but without the server.
		</Abstract>
		<Keywords>
			WFS, WMS, COGJ, GEO-SERVERLESS
		</Keywords>
		<OnlineResource>
			{URL_ROOT}
		</OnlineResource>
		<Fees>
			NONE
		</Fees>
		<AccessConstraints>
			NONE
		</AccessConstraints>
	</Service>
	<Capability>
		<Request>
			<GetCapabilities>
				<DCPType>
					<HTTP>
						<Get onlineResource="{URL_ROOT}?COGJ_URL={COGJ_URL}&amp;REQUEST=GetCapabilities" />
					</HTTP>
				</DCPType>
			</GetCapabilities>
			<DescribeFeatureType>
				<SchemaDescriptionLanguage>
					<XMLSCHEMA />
				</SchemaDescriptionLanguage>
				<DCPType>
					<HTTP>
						<Get onlineResource="{URL_ROOT}?COGJ_URL={COGJ_URL}&amp;REQUEST=DescribeFeatureType" />
					</HTTP>
				</DCPType>
			</DescribeFeatureType>
			<GetFeature>
				<ResultFormat>
					<GML3 />
				</ResultFormat>
				<DCPType>
					<HTTP>
						<Get onlineResource="{URL_ROOT}?COGJ_URL={COGJ_URL}&amp;REQUEST=GetFeature" />
					</HTTP>
				</DCPType>
			</GetFeature>
		</Request>
	</Capability>
	<FeatureTypeList>
		<Operations>
			<Query />
		</Operations>
	</FeatureTypeList>
	<Filter_Capabilities>
		<Spatial_Capabilities>
			<Spatial_Operators>
				<BBOX />
			</Spatial_Operators>
		</Spatial_Capabilities>
		<Scalar_Capabilities>
			<Logical_Operators />
			<Comparison_Operators>
				<Simple_Comparisons />
			</Comparison_Operators>
			<Arithmetic_Operators />
		</Scalar_Capabilities>
	</Filter_Capabilities>
</WFS_Capabilities>
