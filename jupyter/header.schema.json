{"definitions": {}, "$schema": "http://json-schema.org/draft-07/schema#", "$id": "https://github.com/boundlessgeo/coj-spec/spec/header_schema.json", "type": "object", "title": "Cloud Optimized GeoJSON schema", "additionalProperties": true, "required": ["size", "features", "bbox", "collections"], "properties": {"size": {"$id": "#/properties/size", "type": "integer", "title": "The Size Schema", "default": 0, "examples": [102429060]}, "features": {"$id": "#/properties/features", "type": "integer", "title": "The Features Schema", "default": 0, "examples": [124672]}, "name": {"$id": "#/properties/name", "type": "string", "title": "The Name Schema", "default": "", "examples": ["Buildings"], "pattern": "^(.*)$"}, "description": {"$id": "#/properties/description", "type": "string", "title": "The Description Schema", "default": "", "examples": ["Building data from Harford County, MD"], "pattern": "^(.*)$"}, "version": {"$id": "#/properties/version", "type": "string", "title": "The Version Schema", "default": "", "examples": ["1.0.0"], "pattern": "^(.*)$"}, "published": {"$id": "#/properties/published", "type": "string", "title": "The Published Schema", "default": "", "examples": ["2018-09-13T15:05:16+00:00"], "pattern": "^(.*)$"}, "extended_metadata": {"$id": "#/properties/extended_metadata", "type": "object", "title": "The Extended_metadata Schema", "default": null, "required": ["start", "size"], "properties": {"start": {"$id": "#/properties/extended_metadata/properties/start", "type": "integer", "title": "The Start Schema", "default": 0, "examples": [52462]}, "size": {"$id": "#/properties/extended_metadata/properties/size", "type": "integer", "title": "The Size Schema", "default": 0, "examples": [14575]}}}, "bbox": {"$id": "#/properties/bbox", "type": "array", "title": "The Bbox Schema", "items": {"$id": "#/properties/bbox/items", "type": "number", "title": "The Items Schema", "minItems": 4, "maxItems": 4, "additionalItems": false, "default": 0.0, "examples": [-76.57139851406845, 39.39485141810742, -76.08362813450836, 39.72564001999409]}}, "tiles": {"$id": "#/properties/collections", "type": "array", "title": "The Collections Schema", "items": {"$id": "#/properties/collections/items", "type": "object", "description": "a list of collections contained in this file", "title": "The Items Schema", "required": ["start", "size", "bbox", "features"], "properties": {"start": {"$id": "#/properties/collections/items/properties/start", "type": "integer", "title": "The Start Schema", "default": 0, "description": "the byte in the file where the collection starts", "examples": [10240]}, "size": {"$id": "#/properties/collections/items/properties/size", "type": "integer", "description": "the size of the collection in bytes", "title": "The Size Schema", "default": 0, "examples": [327634]}, "bbox": {"$id": "#/properties/collections/items/properties/bbox", "type": "array", "title": "The Bbox Schema", "items": {"$id": "#/properties/collections/items/properties/bbox/items", "type": "number", "title": "The Items Schema", "description": "the WGS84 bounding box of the feature collection", "minItems": 4, "maxItems": 4, "additionalItems": false, "default": 0.0, "examples": [-76.14459943195337, 39.47754856857909, -76.08362813450836, 39.518897143814925]}}, "name": {"$id": "#/properties/collections/items/properties/name", "type": "string", "title": "The Name Schema", "default": "", "examples": ["Buildings"], "pattern": "^(.*)$"}, "description": {"$id": "#/properties/collections/items/properties/description", "type": "string", "title": "The Description Schema", "default": "", "examples": ["A subset of data from from Harford County, MD -- between main street and brodway"], "pattern": "^(.*)$"}, "features": {"$id": "#/properties/collections/items/properties/features", "type": "integer", "title": "The Features Schema", "description": "the number of features in the collection", "default": 0, "examples": [329]}}}}}}